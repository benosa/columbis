                <select id="claim_fuel_tax_count" name="claim[fuel_tax_count]">
                  <option value="{{fuel_tax_count}}" selected="selected">{{fuel_tax_count}}</option>
                </select>
              </td>
            </tr>
            <tr class='countable'>
              <th colspan='6' style='text-align:right; padding-right:6px;'>
                <label for="claim_additional_services_price">Стоимость дополнительных услуг</label>
              </th>
              <td class='amount'><input id="claim_additional_services_price" name="claim[additional_services_price]" size="30" type="text" value="{{additional_services_price}}" /></td>
              <td>
                <select id="claim_additional_services_price_currency" name="claim[additional_services_price_currency]">
                <option value="{{additional_services_price_currency}}"
                  selected="selected">{{additional_services_price_currency_up}}</option>
                </select>
              </td>
              <td></td>
            </tr>
            <tr>
              <td colspan='9'>
                <br>
              </td>
            </tr>
            <tr>
              <th colspan='9'>
                <label for="claim_primary_currency_price">Полная стоимость тура</label>
              </th>
            </tr>
            <tr>
              <td class='amount'><input class="red" id="claim_primary_currency_price" name="claim[primary_currency_price]" size="30" type="text" value="{{primary_currency_price}}" /></td>
              <td colspan='8'>
                <input class="red" disabled="disabled" id="price_as_string" name="price_as_string" type="text" value="{{price_as_string}}" />
              </td>
            </tr>
            <tr>
              <th colspan='9'>
                <label for="claim_calculation">Расчет</label>
              </th>
            </tr>
            <tr>
              <td colspan='9'>
                <textarea cols="40" id="claim_calculation" name="claim[calculation]" rows="4">{{calculation}}</textarea>
              </td>
            </tr>
          </table>
        </table>
      </div>
    </div>
    <div id='payments_block'>
      <div class='data_block'>
        <h2>Оплата туристом</h2>
        <table id='payments_in'>
          <tr>
            <td colspan='2'>
              <label for="claim_maturity">Срок оплаты</label>
            </td>
            <td class='datepicker'><input class="datepicker" id="claim_maturity" name="claim[maturity]" size="30" type="text" value="{{maturity}}" /></td>
            <td></td>
          </tr>
          <tr class='header'>
            <th style='width:12px;'></th>
            <th>Дата</th>
            <th>Оплачено</th>
            <th>Валюта</th>
            <th>Сумма прописью</th>
            <th>Форма оплаты</th>
          </tr>
          {{#payments_in}}
          <tr class='fields' id='payment_in_{{n}}'>
            <td><input name="claim[payments_in_attributes][{{n}}][approved]" type="hidden" value="0" /><input class="approved" id="claim_payments_in_attributes_{{n}}_approved" name="claim[payments_in_attributes][{{n}}][approved]" type="checkbox" value="1" {{#approved}}checked="checked"{{/approved}} /></td>
            <td class='datepicker'><div class="field_with_errors"><input class="datepicker date_in" id="claim_payments_in_attributes_{{n}}_date_in" name="claim[payments_in_attributes][{{n}}][date_in]" size="30" type="text" value="{{date_in}}" /></div></td>
            <td class='amount'><input class="amount" id="claim_payments_in_attributes_{{n}}_amount" name="claim[payments_in_attributes][{{n}}][amount]" size="30" type="text" value="{{amount}}" /></td>
            <td>
              <select class="currency" disabled="disabled" id="claim_payments_in_attributes_{{n}}_currency" name="claim[payments_in_attributes][{{n}}][currency]">
                <option value="{{currency}}" selected="selected">{{currency_up}}</option>
              </select>
            </td>
            <td><input class="description" disabled="disabled" id="claim_payments_in_attributes_{{n}}_description" name="claim[payments_in_attributes][{{n}}][description]" size="30" type="text" value="{{description}}" /></td>
            <td colspan='2'>
              <div class="field_with_errors"><select class="payment_form" id="claim_payments_in_attributes_{{n}}_form" name="claim[payments_in_attributes][{{n}}][form]">
                <option value="{{form}}" selected="selected">{{form}}</option>
              </select></div>
            </td>
            <td>{{#can_edit}}<a href="#" class="del" id="del{{n}}">X</a>{{/can_edit}}</td>
          </tr>
          <input id="claim_payments_in_attributes_{{n}}_id" name="claim[payments_in_attributes][{{n}}][id]" type="hidden" />
          {{/payments_in}}
          {{^payments_in}}
          <tr class='fields' id='payment_in_0'>
            <td><input name="claim[payments_in_attributes][0][approved]" type="hidden" value="0" /><input class="approved" id="claim_payments_in_attributes_0_approved" name="claim[payments_in_attributes][0][approved]" type="checkbox" value="1" /></td>
            <td class='datepicker'><div class="field_with_errors"><input class="datepicker date_in" id="claim_payments_in_attributes_0_date_in" name="claim[payments_in_attributes][0][date_in]" size="30" type="text" /></div></td>
            <td class='amount'><input class="amount" id="claim_payments_in_attributes_0_amount" name="claim[payments_in_attributes][0][amount]" size="30" type="text" value="0.0" /></td>
            <td>
              <select class="currency" disabled="disabled" id="claim_payments_in_attributes_0_currency" name="claim[payments_in_attributes][0][currency]">
                <option value="{{currency}}" selected="selected">{{currency_up}}</option>                
              </select>
            </td>
            <td><input class="description" disabled="disabled" id="claim_payments_in_attributes_0_description" name="claim[payments_in_attributes][0][description]" size="30" type="text" /></td>
            <td colspan='2'>
              <div class="field_with_errors"><select class="payment_form" id="claim_payments_in_attributes_0_form" name="claim[payments_in_attributes][0][form]"><option value="Безналичный">Безналичный</option>
              <option value="Наличный">Наличный</option></select></div>
            </td>
            <td>{{#can_edit}}<a href="#" class="del" id="del0">X</a>{{/can_edit}}</td>
          </tr>
          <input id="claim_payments_in_attributes_0_id" name="claim[payments_in_attributes][0][id]" type="hidden" />
          {{/payments_in}}
          <tr class='footer'>
            <td colspan='3'></td>
            <td><label for="claim_tourist_debt">Долг туристов</label></td>
            <td><input class="red" disabled="disabled" id="claim_tourist_debt" name="claim[tourist_debt]" size="30" type="text" value="{{tourist_debt}}" /></td>
          </tr>
          {{#can_edit}}
          <tr class='bottom'>
            <td colspan='5'>
              <a href="#" class="add">Добавить платеж</a>
            </td>
          </tr>
          {{/can_edit}}
        </table>
        <h2>Оплата оператору</h2>
        <table id='payments_out'>
          <tr>
            <td></td>
            <td><label for="claim_operator_price">К оплате</label></td>
            <td class='amount'><input id="claim_operator_price" name="claim[operator_price]" size="30" type="text" value="{{operator_price}}" /></td>
            <td>
              <select id="claim_operator_price_currency" name="claim[operator_price_currency]">
                <option value="{{operator_price_currency}}" selected="selected">{{operator_price_currency_up}}</option>
              </select>
            </td>
            <td colspan='2'>
              <label for="claim_operator_maturity">Срок оплаты оператору</label>
            </td>
            <td class='datepicker'><input class="datepicker" id="claim_operator_maturity" name="claim[operator_maturity]" size="30" type="text" value="{{operator_maturity}}" /></td>
          </tr>
          <tr class='header'>
            <th style='width:12px;'></th>
            <th>Дата</th>
            <th>Оплачено</th>
            <th>Валюта</th>
            <th>Курс</th>
            <th class='paid'>Оплачено (rur)</th>
            <th>Форма оплаты</th>
          </tr>
          {{#payments_out}}
          <tr class='fields' id='payment_out_{{n}}'>
            <td><input name="claim[payments_out_attributes][{{n}}][approved]" type="hidden" value="0" /><input class="approved" id="claim_payments_out_attributes_{{n}}_approved" name="claim[payments_out_attributes][{{n}}][approved]" title="Подтверждено" type="checkbox" value="1" {{#approved}}checked="checked"{{/approved}} /></td>
