= form_for @claim do |f|
  = f.error_messages
  %p
    = f.label :tourist_id
    %br
    = f.select :tourist_id, Tourist.all.collect {|o| [ o.name, o.id ] }
  %p
    = f.label :user_id
    %br
    = f.select :user_id, User.all.collect {|o| [ o.username, o.id ] }
  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.label :check_date
    %br
    = f.text_field :check_date
  %p
    = f.submit

