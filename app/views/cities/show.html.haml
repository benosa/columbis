- title @city.name

- if @city.country
  %p
    = @city.country.name
    
- content_for(:float_panel) do
  = link_to t('view_all'), cities_path, :class => 'list_operator'
  = link_to t('edit'), edit_city_path(@city), :class => 'add_operator'
  = link_to t('destroy'), @city, :confirm => t('are_you_sure'), :method => :delete, :class => 'delete_operator'