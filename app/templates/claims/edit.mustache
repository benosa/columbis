            <td class='amount'><input class="amount" id="claim_payments_out_attributes_{{n}}_amount" name="claim[payments_out_attributes][{{n}}][amount]" size="30" type="text" value="{{amount}}" /></td>
            <td>
              <select class="currency" disabled="disabled" id="claim_payments_out_attributes_{{n}}_currency" name="claim[payments_out_attributes][{{n}}][currency]">
                <option value="{{currency}}" selected="selected">{{currency_up}}</option>
              </select>
            </td>
            <td class='amount'>
              <input class="course" id="claim_payments_out_attributes_{{n}}_course" name="claim[payments_out_attributes][{{n}}][course]" size="30" type="text" value="{{course}}" />
            </td>
            <td class='amount'><input class="amount_prim" id="claim_payments_out_attributes_{{n}}_amount_prim" name="claim[payments_out_attributes][{{n}}][amount_prim]" size="30" type="text" value="{{amount_prim}}" /></td>
            <td>
              <div class="field_with_errors"><select class="payment_form" id="claim_payments_out_attributes_{{n}}_form" name="claim[payments_out_attributes][{{n}}][form]">
                <option value="{{form}}" selected="selected">{{form}}</option>
              </select></div>
            </td>
            <td>{{#can_edit}}<a href="#" class="del" id="del{{n}}">X</a>{{/can_edit}}</td>
          </tr>
          <input id="claim_payments_out_attributes_{{n}}_id" name="claim[payments_out_attributes][{{n}}][id]" type="hidden" />
          {{/payments_out}}
          {{^payments_out}}
          <tr class='fields' id='payment_out_0'>
            <td><input name="claim[payments_out_attributes][0][approved]" type="hidden" value="0" /><input class="approved" id="claim_payments_out_attributes_0_approved" name="claim[payments_out_attributes][0][approved]" title="Подтверждено" type="checkbox" value="1" /></td>
            <td class='datepicker'><div class="field_with_errors"><input class="datepicker date_in" id="claim_payments_out_attributes_0_date_in" name="claim[payments_out_attributes][0][date_in]" size="30" type="text" /></div></td>
            <td class='amount'><input class="amount" id="claim_payments_out_attributes_0_amount" name="claim[payments_out_attributes][0][amount]" size="30" type="text" value="0.0" /></td>
            <td>
              <select class="currency" disabled="disabled" id="claim_payments_out_attributes_0_currency" name="claim[payments_out_attributes][0][currency]">
                <option value="{{currency}}" selected="selected">{{currency_up}}</option>
              </select>
            </td>
            <td class='amount'>
              <input class="course" id="claim_payments_out_attributes_0_course" name="claim[payments_out_attributes][0][course]" size="30" type="text" value="1.0" />
            </td>
            <td class='amount'><input class="amount_prim" id="claim_payments_out_attributes_0_amount_prim" name="claim[payments_out_attributes][0][amount_prim]" size="30" type="text" value="0.0" /></td>
            <td>
              <div class="field_with_errors"><select class="payment_form" id="claim_payments_out_attributes_0_form" name="claim[payments_out_attributes][0][form]"><option value="Безналичный">Безналичный</option>
              <option value="Наличный">Наличный</option></select></div>
            </td>
            <td>{{#can_edit}}<a href="#" class="del" id="del0">X</a>{{/can_edit}}</td>
          </tr>
          <input id="claim_payments_out_attributes_0_id" name="claim[payments_out_attributes][0][id]" type="hidden" />
          {{/payments_out}}
          <tr class='footer'>
            <td colspan='3'></td>
            <td><label for="claim_operator_debt">Долг оператору</label></td>
            <td colspan='2'><input class="red" disabled="disabled" id="claim_operator_debt" name="claim[operator_debt]" size="30" type="text" value="{{operator_debt}}" /></td>
          </tr>
          {{#can_edit}}
          <tr class='bottom'>
            <td colspan='6'>
              <a href="#" class="add">Добавить платеж</a>
            </td>
          </tr>
          {{/can_edit}}
        </table>
      </div>
    </div>
  </form>
