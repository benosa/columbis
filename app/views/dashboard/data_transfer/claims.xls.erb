  <Worksheet ss:Name="Операторы">
    <Table>
      <Row>
        <Cell><Data ss:Type="String"><%= t('activerecord.attributes.operator.name') %></Data></Cell>
        <Cell><Data ss:Type="String"><%= t('activerecord.attributes.operator.full_name') %></Data></Cell>
        <Cell><Data ss:Type="String"><%= t('activerecord.attributes.operator.register_number') %></Data></Cell>
        <Cell><Data ss:Type="String"><%= t('activerecord.attributes.operator.register_series') %></Data></Cell>
